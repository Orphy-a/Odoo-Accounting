<odoo>
    <record id="action_custom_account_move" model="ir.actions.act_window">
        <field name="name">Journal Entries</field>
        <field name="res_model">custom.account.move</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a new journal entry
            </p>
        </field>
    </record>

    <record id="view_custom_account_move_tree" model="ir.ui.view">
        <field name="name">custom.account.move.tree</field>
        <field name="model">custom.account.move</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="date"/>
                </tree>
        </field>
    </record>

    <record id="view_custom_account_move_form" model="ir.ui.view">
        <field name="name">custom.account.move.form</field>
        <field name="model">custom.account.move</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="date"/>
                        </group>
                </sheet>
            </form>
        </field>
    </record>

    <menuitem id="menu_custom_account_move"
              name="Journal Entries"
              parent="menu_accounting_root"
              action="action_custom_account_move"
              sequence="30"/> </odoo>